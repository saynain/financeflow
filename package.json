{
  "name": "financeflow",
  "version": "1.0.0",
  "private": true,
  "description": "A modern budget and expense planning web application",
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "clean": "turbo clean && rm -rf node_modules",
    "db:generate": "turbo db:generate",
    "db:push": "turbo db:push",
    "db:migrate": "cd packages/database && dotenv -e ../../.env -- npx prisma migrate dev",
    "db:studio": "cd packages/database && dotenv -e ../../.env -- npx prisma studio",
    "setup": "./setup.sh",
    "fix-env": "./fix-env.sh"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "dotenv-cli": "^7.4.1",
    "prettier": "^3.2.4",
    "turbo": "^1.11.3",
    "typescript": "^5.3.3"
  }
}
